{
  "expo": {
    "name": "picshare",
    "slug": "picshare",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "fr.picnshare.app",
      "buildNumber": "0.1.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take pictures that will be shared and displayed to other users of the app",
        "NSLocationWhenInUseUsageDescription": "This app uses the location when in use to better recommend events near the user"
      }
    },
    "android": {
      "package": "fr.picnshare.app",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["ACCESS_FINE_LOCATION", "CAMERA"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
